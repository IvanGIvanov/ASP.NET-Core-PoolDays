@model AddPoolFormModel

@{
    ViewData["Title"] = "Add Pool In PoolDays";
}

<form method="post">
    <div class="form-group">
        <label asp-for="Manufacturer">Manufacturer</label>
        <input class="form-control" placeholder="Intex">
    </div>
    <div class="form-group">
        <label asp-for="Model">Model</label>
        <input class="form-control" placeholder="26329EH">
    </div>
    <div class="form-group">
        <label asp-for="Description">Description</label>
        <textarea class="form-control" rows="3" placeholder="Enter description up to 3000 symbols"></textarea>
    </div>
    <div class="form-group">
        <label asp-for="Volume">Volume</label>
        <input class="form-control" placeholder="Enter number value between 1 and 199">
    </div>
    <div class="form-group">
        <label asp-for="Height">Height</label>
        <input class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Width">Width</label>
        <input class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Depth">Depth</label>
        <input class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Diameter">Diameter</label>
        <input class="form-control">
    </div>
    <div class="form-check">
        <label asp-for="PumpIncluded">
            <input type="checkbox" class="form-check-input" value="">Pump Included
        </label>
    </div>
    <div class="form-check">
        <label asp-for="Stairway">
            <input type="checkbox" class="form-check-input" value="">Stairway
        </label>
    </div>
    <div class="form-group">
        <label asp-for="ImageUrl">ImageUrl</label>
        <input class="form-control">
    </div>
    <div class="form-group">
        <label for="exampleFormControlSelect1">Example select</label>
        <select class="form-control" id="exampleFormControlSelect1">
            <option>1</option>
            <option>2</option>
            <option>3</option>
            <option>4</option>
            <option>5</option>
        </select>
    </div>
</form>